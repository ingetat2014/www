{% trans_default_domain 'FOSUserBundle' %}
<br/>
<div class="fos_user_user_show">
    <div class="span8"> 
    <div class="row-fluid">         
        <div class="span4 text-left ">Nom d'utilisateur :</div>
        <div class="span4 text-left badge">{{ user.username }}</div>
    </div>   
    <div class="row-fluid">  
        <div class="span4 text-left">Adresse Email :</div>
        <div class="span4 text-left badge">{{ user.email }}</div>
    </div>
    <div class="row-fluid">  
        <div class="span4 text-left">Role(s) :</div>
        <div class="span4 text-left">{{ user.roles[0] }}</div>
    </div>
   <div class="row-fluid">  
        <div class="span4 text-left">Nom :</div>
        <div class="span4 text-left">{{ user.nom }}</div>
   </div> 
   <div class="row-fluid">  
        <div class="span4 text-left">Prénom :</div>
        <div class="span4 text-left">{{ user.prenom }}</div>
   </div>    
   <div class="row-fluid">       
        <div class="span4 text-left"> Matricule :</div>
        <div class="span4 text-left">{{ user.matricule }}</div>
   </div>
   <div class="row-fluid">       
        <div class="span4 text-left">CIN :</div>
        <div class="span4 text-left">{{ user.cin }}</div>
   </div>
  <div class="row-fluid">        
        <div class="span4 text-left">Dépot :</div>
        <div class="span4 text-left">{{ user.depot }}</div>
  </div>     
    </div>
    </div>
</div>
